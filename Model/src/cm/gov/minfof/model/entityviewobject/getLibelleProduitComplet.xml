  <SQLQuery><![CDATA[select partiesproduitspfnl.idpartiesproduitspfnl, concat(produitspfnl.nomcommercial, '(', partiesrecoltees.nompartierecoltee, ')') as libelle 
from produitspfnl
inner join (partiesrecoltees, partiesproduitspfnl)
on (produitspfnl.idproduitspfnl = partiesproduitspfnl.idproduitspfnl and 
	   partiesrecoltees.idpartiesrecoltees = partiesproduitspfnl.idpartiesrecoltees)
order by 1]]></SQLQuery>
